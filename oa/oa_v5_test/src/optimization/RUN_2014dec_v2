[ -d $WORK_DIR ] || mkdir -p $WORK_DIR

cp *py *sh $WORK_DIR

pushd $WORK_DIR

$PYTHON auctionFastWithSubcatConstraintsV3E3.py scoreFile offerFile memberFile offer2subcatFile > RUN.log
